{
  "name": "stupid-tris",
  "version": "0.0.1",
  "description": "Another implementation of the Tris game",
  "private": true,
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^3.4.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "gulp-cli": "^2.2.0",
    "jquery": "^3.4.1",
    "jquery.cookie": "^1.4.1",
    "popper.js": "^1.14.3"
  },
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jquery": "^1.3.1",
    "gulp": "^4.0.2",
    "style-loader": "^0.20.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^2.0.13"
  },
  "scripts": {
    "build": "docker build -t stupid-tris .",
    "start-cmd": "docker run -d -v %cd%/www:/var/www/html -p 127.0.0.1:80:80 --name running-stupid-tris stupid-tris && npm run webpack",
    "start-bash": "docker run -d -v ./www:/var/www/html -p 127.0.0.1:80:80 --name running-stupid-tris stupid-tris && npm run webpack",
    "clean": "docker stop running-stupid-tris && docker rm running-stupid-tris",
    "webpack": "webpack -d --optimize-minimize --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/riccardo-giovarelli/stupid-tris.git"
